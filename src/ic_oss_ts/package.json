{
  "name": "@ldclabs/ic_oss_ts",
  "version": "0.7.0",
  "type": "module",
  "description": "The Typescript version of the client SDK for the ic-oss cluster.",
  "license": "MIT",
  "homepage": "https://github.com/ldclabs/ic-oss",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ldclabs/ic-oss.git"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "browser": {
    "node:fs/promises": false,
    "node:path": false,
    "mime/lite": false
  },
  "files": [
    "candid",
    "dist",
    "src",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "prebuild": "dfx generate && rm -rf candid && mv '../declarations' './candid'",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\"",
    "test": "vitest run",
    "ncu": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@dfinity/agent": "^2.0.0",
    "@dfinity/candid": "^2.0.0",
    "@dfinity/principal": "^2.0.0",
    "@dfinity/utils": "^2.4.0",
    "@noble/hashes": "^1.4.0",
    "mime": "^4.0.4"
  },
  "devDependencies": {
    "@types/node": "22.5.0",
    "@types/eslint": "^9.6.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "typescript": "^5.5.4",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "vitest": "^2.0.5"
  }
}